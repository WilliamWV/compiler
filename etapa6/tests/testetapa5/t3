/**
	Teste de operações relacionais:
		==
		!=
		<	
		>
		<=
		>=
*/
a int;
b int;
c int;
int main(){
	int x1;
	int x2;
	int x3;
	int x4;
	int x5;
	int x6;

	a = 1;
	b = 2;
	c = 2;
	if (a > b) then {
		x1 = 1;
	}
	else{
		x1 = 0;
	};
	if (a < b) then {
		x2 = 1;
	}
	else{
		x2 = 0;
	};
	if (c >= b) then {
		x3 = 1;
	}
	else{
		x3 = 0;
	};
	if (c <= a) then {
		x4 = 1;
	}
	else{
		x4 = 0;
	};
	if (a == b) then {
		x5 = 1;
	}
	else{
		x5 = 0;
	};
	if (c != b) then {
		x6 = 1;
	}
	else{
		x6 = 0;
	};
}

/**
	No final esperá-se
		x1 = 0
		x2 = 1
		x3 = 1
		x4 = 0
		x5 = 0
		x6 = 0
*/
